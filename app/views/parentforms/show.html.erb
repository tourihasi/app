<head>
<title>帳票詳細一覧</title>
</head>
<body>
<button class='btn'><%=link_to '更新',new_parentform_childform_path(params[:id])%></button>
<table class='table_common'>
  <thead>
    <th>項目名</th>
    <th>登録者</th>
    <th>改定符号/リヴィジョン</th>
    <th>改定理由</th>
    <th>改定日</th>
    <th>更新日</th>
  </thead>
  <tbody>
  <% @childforms.each do |child| %>
  <tr>
    <td><<%=@parentform.name%></td>
    <td><<%=child.user.name%></td>
    <td><<%=child.revision%></td>
    <td><<%=child.reson%></td>
    <td><<%=cild.created_at%></td>
    <td><<%=child.updates_at%></td>
  <%end%>
  </tbody>
</table>
</body>